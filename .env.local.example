
# Firebase Project Configuration
# Copy these values from your Firebase project settings
# (Project settings > General > Your apps > Web app > SDK setup and configuration)
# IMPORTANT: After creating/modifying .env.local, you MUST restart your Next.js development server.

NEXT_PUBLIC_FIREBASE_API_KEY=YOUR_ACTUAL_FIREBASE_API_KEY_HERE
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=YOUR_ACTUAL_FIREBASE_AUTH_DOMAIN_HERE
NEXT_PUBLIC_FIREBASE_PROJECT_ID=YOUR_ACTUAL_FIREBASE_PROJECT_ID_HERE
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=YOUR_ACTUAL_FIREBASE_STORAGE_BUCKET_HERE
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=YOUR_ACTUAL_FIREBASE_MESSAGING_SENDER_ID_HERE
NEXT_PUBLIC_FIREBASE_APP_ID=YOUR_ACTUAL_FIREBASE_APP_ID_HERE
# Optional: Only if you use Firebase Analytics
# NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID=YOUR_ACTUAL_FIREBASE_MEASUREMENT_ID_HERE

# Google Maps API Key (if you are using Google Maps JavaScript API directly)
# Ensure this key is properly restricted in your Google Cloud Console for security.
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=YOUR_ACTUAL_GOOGLE_MAPS_API_KEY_HERE
